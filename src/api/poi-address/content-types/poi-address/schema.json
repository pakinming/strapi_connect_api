{
  "kind": "collectionType",
  "collectionName": "poi_addresses",
  "info": {
    "singularName": "poi-address",
    "pluralName": "poi-addresses",
    "displayName": "poi_address"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "address_line": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "district": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "province": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "country": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "postal_code": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "geo_location": {
      "type": "customField",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "customField": "plugin::geodata.geojson",
      "options": {
        "info": true
      }
    },
    "address": {
      "type": "customField",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "customField": "plugin::address-selection.address"
    },
    "demo_date": {
      "type": "customField",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "customField": "plugin::strapi-date-range-picker-5.date-range-picker"
    },
    "Date": {
      "type": "date",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "date_time": {
      "type": "datetime",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "time": {
      "type": "time",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    }
  }
}
